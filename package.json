{
  "name": "gateway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "precommit": "lint-staged",
    "build": "nest build",
    "format": "prettier --write \"**/*.ts\"",
    "start": "node dist/src/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "migration:generate": "ts-node node_modules/sequelize-cli/lib/sequelize migration:generate --name",
    "migration:up": "ts-node node_modules/sequelize-cli/lib/sequelize db:migrate",
    "migration:down": "ts-node node_modules/sequelize-cli/lib/sequelize db:migrate:undo",
    "seed:generate": "ts-node node_modules/sequelize-cli/lib/sequelize seed:generate --name",
    "seed:up": "ts-node node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "seed:down": "ts-node node_modules/sequelize-cli/lib/sequelize db:seed:undo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){process.exit(1)} \" || husky install"
  },
  "engines": {
    "node": "16.15.0"
  },
  "dependencies": {
    "@casl/ability": "^6.0.0",
    "@fastify/static": "^5.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-fastify": "^8.3.1",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/typeorm": "^8.0.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.1",
    "express-session": "^1.17.3",
    "fastify-helmet": "^7.0.1",
    "fastify-multer": "^2.0.2",
    "fastify-swagger": "^4.15.0",
    "multer": "^1.4.5-lts.1",
    "nestjs-pino": "^3.1.1",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.3",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pino-http": "^8.2.0",
    "pino-pretty": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.21.3",
    "sequelize-typescript": "^2.1.3",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.1.0",
    "@jackfranklin/test-data-bot": "^2.0.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jest": "27.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport": "^1.0.8",
    "@types/pbkdf2": "^3.1.0",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.0.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "faker": "^6.6.6",
    "husky": "^8.0.1",
    "jest": "^27.2.5",
    "jest-mock-extended": "^2.0.6",
    "lint-staged": "^13.0.3",
    "mock": "^0.1.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.3",
    "sequelize-cli": "^6.3.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix --ext .ts",
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ]
  }
}
